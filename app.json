{
    "expo": {
        "name": "Hacknode",
        "slug": "hacknode",
        "version": "1.1.0",
        "orientation": "portrait",
        "icon": "./assets/hashnode.png",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#ffffff"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": ["**/*"],
        "ios": {
            "supportsTablet": true
        },
        "android": {
            "adaptiveIcon": {
                "foregroundImage": "./assets/hashnode.png",
                "backgroundColor": "#FFFFFF"
            },
            "package": "com.dephraiim.hacknode"
        }
    }
}
